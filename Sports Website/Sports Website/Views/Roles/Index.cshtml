@using Microsoft.AspNetCore.Identity;
@model List<IdentityRole>
@{
    ViewData["Title"] = "Roles";
}
@Html.ActionLink("New Role" , "Insert" , "Roles" , null , new{@class = "btn btn-primary"})
<table class="table">
    <thead>
        <tr>
            <th scope="col">Id</th>
            <th scope="col">Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var role in Model)
        {
            <tr>
                <th>@role.Id</th>
                <td>@role.Name</td>

            </tr>
        }
        
        
    </tbody>
</table>

<script src="~/js/Roles.js"></script>